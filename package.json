{
  "name": "premid-bot",
  "description": "Discord Bot for PreMiD",
  "repository": "https://github.com/PreMiD/Discord-Bot",
  "version": "2.1.1",
  "scripts": {
    "compile": "tsc && tsc devMode && node devMode.js && cd dist && npm i --production --prefer-offline",
    "dev": "nodemon --quiet --exec 'node devMode.js & cd dist && node -r source-map-support/register index.js' -w dist -e js"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "2.4.2",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "8.1.0",
    "fast-glob": "3.0.4",
    "fs-extra": "8.1.0",
    "mongodb": "3.3.2",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/mongodb": "3.3.1",
    "@types/request-promise-native": "1.0.16",
    "nodemon": "1.19.1",
    "source-map-support": "0.5.13",
    "typescript": "3.5.3"
  }
}
