{
	"compilerOptions": {
		"target": "ESNEXT",
		"module": "commonjs",
		"rootDir": "./src",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"]
}
